use std::{io,str::{FromStr},error};
use io::{Read,BufRead,BufWriter};

#[derive(Debug)]
pub enum CodeError<T: FromStr> {
    Io(io::Error),
    Parse(<T as FromStr>::Err),
    Misc(error::Error),
}

#[derive(Debug)]
struct Scanner<R: BufRead> {
    reader: R,
    buffer: Vec<u8>
}


fn main() {
    println!("Hello World!")
}
